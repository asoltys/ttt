@{
    if(Culture == "fr")
    {
        Layout = "~/Views/Shared/_IntraLayoutFra.cshtml";
        ViewBag.Title = "Calendrier national des transformations";
        ViewBag.lang = "fra";
    }
    else
    {
        Layout = "~/Views/Shared/_IntraLayout.cshtml";
        ViewBag.Title = "National Transformation Timeline Tool";
        ViewBag.lang = "eng";
    }
}

<div id="timeline-tool">
    <div id="timeline-tool-intro">
        <div id="about" class="span-7 margin-bottom-none"></div>
        <div id="legend" class="span-1 margin-bottom-none"></div>
        <div class="clear"></div>
    </div>

    <div id="navHeaderContainer" class="grid-12">
        <div id="navHeader" class="span-12">
            <div id="leftNav" class="span-1 margin-left-none margin-bottom-none margin-top-large"></div>
            <div id="viewNav" class="span-3 margin-bottom-none margin-top-large"></div>
            <div id="areaNav" class="span-2 margin-bottom-none margin-top-large"></div>
            <div id="branchNav" class="span-5 margin-bottom-none margin-top-large"></div>
            <div id="rightNav" class="span-1 margin-right-none margin-bottom-none margin-top-large"></div>
            <div id="dateNav" class="span-12" style="margin: 0px; overflow: hidden;"></div>
        </div>
    </div>
    <div class="clear"></div>
    <div id="navHeaderSpace"></div>
    <div id="render" class="span-8" style=""></div>
    <div id="dialog" title="dialog"></div>
</div>

@section Scripts {
<link rel="stylesheet" href="/Content/jquery-ui.min.css">
<link rel="stylesheet" type="text/css" media="screen,print" href="/Content/master.css" />
<link rel="stylesheet" type="text/css" media="screen" href="/Content/screen.css" />
<link rel="stylesheet" type="text/css" media="print" href="/Content/print.css" />
<script src="/Scripts/jquery-ui.min.js"></script>
<script src="/Scripts/server_settings.js"></script>
<script src="/Scripts/translation.js"></script>
<script src="/Scripts/tl_utility.js"></script>
<script src="/Scripts/will.js"></script>
<script src="/Scripts/moment-with-locales.min.js"></script>

}
